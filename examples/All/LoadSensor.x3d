      </Appearance>
      <Sphere radius='0.1' />
    </Shape>

    <LoadSensor DEF='S'>
      <ImageTexture USE='I' containerField='watchList' />
    </LoadSensor>
  
    <!--ROUTE
        fromNode='S' fromField=''
